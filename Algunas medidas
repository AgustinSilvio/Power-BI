IntervaloEdad = 
/* Asigna un intervalo de edad a cada registros. No se toman en cuenta las fechas de nacimiento anteriores al 1/01/1920 y posteriores al 31/12/2024, aquellas, las que sean cero, no sean un número y estén vacias, van a ser asginadas al intervalo "Edad no válida"*/
IF(
    ISBLANK(ANEXO_EXP_AP_VG_SALUD[EDAD]) || 
    NOT(ISNUMBER(ANEXO_EXP_AP_VG_SALUD[EDAD])) || 
    ANEXO_EXP_AP_VG_SALUD[EDAD] = 0,
    "Edad no válida",
    SWITCH(
        TRUE(),
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 14, "0 - 14",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 25, "15 - 25",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 35, "26 - 35",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 45, "36 - 45",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 55, "46 - 55",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 60, "56 - 60",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 65, "61 - 65",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 70, "66 - 70",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 75, "71 - 75",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 80, "76 - 80",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 85, "81 - 85",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 90, "86 - 90",
        ANEXO_EXP_AP_VG_SALUD[EDAD] <= 95, "91 - 95",
        "96 +" ) )


Medidas dinamicas:
Medidas % AP_VG = --Se crea para intentar que el usuario pueda cambiar dinamicamente entre medidas cuando mire las matrices de AP_VG_Salud, para que mantenga el formato de los datos
SWITCH(
    SELECTEDVALUE('Medidas_%_Seleccionadas'[Medida]),
    "% Exps", [Formato_% Expuestos AP_VG],
    "% SA", [Formato_%_SA_total_AP_VG],
    "% Prima", [Formato_%_Prima_Anual_AP_VG] )
con:
Formato_% Expuestos AP_VG = FORMAT([% Expuestos AP_VG], "0.00%") --por ejemplo

